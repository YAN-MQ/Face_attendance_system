\r\n            <v-row>\r\n              <v-col cols=\"12\" md=\"7\">\r\n                <div class=\"camera-container\">\r\n                  <video ref=\"video\" width=\"100%\" autoplay playsinline></video>\r\n                  <canvas ref=\"canvas\" style=\"display: none;\"></canvas>\r\n                  \r\n                  <div v-if=\"!cameraStarted\" class=\"camera-overlay\">\r\n                    <v-btn color=\"primary\" @click=\"startCamera\">\r\n                      <v-icon left>mdi-camera</v-icon>\r\n                      启动摄像头\r\n                    </v-btn>\r\n                  </div>\r\n                  \r\n                  <div v-if=\"capturedImage\" class=\"preview-overlay\">\r\n                    <img :src=\"capturedImage\" class=\"preview-image\" />\r\n                    <div class=\"preview-actions\">\r\n                      <v-btn color=\"error\" @click=\"cancelCapture\">\r\n                        <v-icon left>mdi-close</v-icon>\r\n                        重新拍照\r\n                      </v-btn>\r\n                    </div>\r\n                  </div>\r\n                  \r\n                  <div v-if=\"isLoading\" class=\"loading-overlay\">\r\n                    <v-progress-circular indeterminate color=\"primary\"></v-progress-circular>\r\n                    <div class=\"mt-2\">处理中...</div>\r\n                  </div>\r\n                </div>\r\n                \r\n                <div class=\"mt-4 text-center\">\r\n                  <v-btn color=\"primary\" @click=\"captureImage\" :disabled=\"!cameraStarted || capturedImage || isLoading\">\r\n                    <v-icon left>mdi-camera</v-icon>\r\n                    拍照\r\n                  </v-btn>\r\n                </div>\r\n              </v-col>\r\n              \r\n              <v-col cols=\"12\" md=\"5\">\r\n                <v-form ref=\"form\" v-model=\"isFormValid\">\r\n                  <v-card outlined>\r\n                    <v-card-title>学生信息</v-card-title>\r\n        